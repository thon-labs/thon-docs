{
  "name": "thon-docs",
  "description": "Document your React components easily",
  "version": "0.0.0",
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "*"
  ],
  "license": "MIT",
  "scripts": {
    "compile": "tsc -p .",
    "build": "rm -rf dist && yarn compile",
    "postbuild": "gulp postBuild && cp package.json dist && cp README.md dist",
    "package": "rimraf *.tgz && cd dist && yarn pack && cd ../ && mv dist/*.tgz ./",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf dist",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@types/jest": "^29.1.2",
    "@types/marked": "^4.0.7",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^8.2.5",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "glob": "^8.0.3",
    "gulp": "^4.0.2",
    "gulp-remove-empty-lines": "^0.1.0",
    "gulp-uglify": "^3.0.2",
    "jest": "^29.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "through2": "^4.0.2",
    "ts-jest": "^29.0.3",
    "tsconfig": "*",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "front-matter": "^4.0.2",
    "marked": "^4.2.3"
  }
}
